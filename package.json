{
  "name": "ngrx-router",
  "version": "2.0.1",
  "description": "Router bindings for NGRX",
  "main": "dist/index.js",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "test": "ngc -p tsconfig.spec.json && jasmine dist/spec/*.spec.js",
    "publish": "npm publish dist",
    "precommit": "lint-staged",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --parser typescript --single-quote es5 --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://amcdnl@github.com/amcdnl/ngrx-router.git"
  },
  "keywords": [
    "ngrx",
    "redux",
    "state",
    "store",
    "state-mangement",
    "angular"
  ],
  "author": "Austin McDaniel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amcdnl/ngrx-router/issues"
  },
  "homepage": "https://github.com/amcdnl/ngrx-router#readme",
  "peerDependencies": {
    "@angular/core": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^6.0.0",
    "@ngrx/store": "^6.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^0.6.5",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@types/jasmine": "^2.8.0",
    "husky": "^0.14.3",
    "jasmine": "^2.8.0",
    "lint-staged": "^7.1.2",
    "ng-packagr": "2.4.5",
    "prettier": "^1.12.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.2.0",
    "tsickle": "^0.29.0",
    "tslib": "^1.9.1",
    "tslint": "^5.8.0",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.4"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  }
}
